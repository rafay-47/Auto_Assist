<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearColumndftwaterp"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_10pxh"
    android:layout_width="400dp"
    android:orientation="vertical">
    <androidx.cardview.widget.CardView
        android:id="@+id/linearRowsubtotalOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:elevation="@dimen/_70pxh"
        android:backgroundTint="@color/white_A700"
        android:padding="10dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtDftwaterpump"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                style="@style/txtPoppinsmedium14_1"
                android:text="@string/msg_dft_water_pump_aw41262"/>
            <View android:background="@color/blue_gray_100"
                android:id="@+id/lineLinesevenOne"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_9pxv" android:layout_width="match_parent"/>
            <LinearLayout
                android:id="@+id/linearRowscreenshot20"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSparePart"
                    android:layout_height="@dimen/_64pxh"
                    android:layout_width="@dimen/_68pxh"
                    android:layout_marginEnd="10dp"
                    android:layout_marginTop="10dp"
                    android:scaleType="fitXY"
                    tools:ignore="ContentDescription" android:background="@drawable/img_screenshot_2021_11_16_64x68"/>
                <LinearLayout
                    android:id="@+id/linearColumnprice"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:id="@+id/linearRowprice"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_6pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            style="@style/txtPoppinsmedium14" android:text="@string/lbl_price"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice1"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_150pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            style="@style/txtPoppinsmedium14" android:text="@string/lbl_pkr_57000"/>
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:id="@+id/linearRowquantity"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtQuantity"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            style="@style/txtPoppinsmedium14" android:text="@string/lbl_quantity"/>
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:id="@+id/linearRoweditminus"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_130pxh"
                            android:layout_width="match_parent" android:orientation="horizontal">

                            <TextView android:ellipsize="end"
                                android:id="@+id/txtOneOne"
                                android:layout_gravity="top"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                style="@style/txtPoppinsmedium16"
                                android:text="@string/lbl_1"/>

                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center_vertical"
                        android:id="@+id/linearRowsubtotal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSubtotal"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            style="@style/txtPoppinsmedium14"
                            android:text="@string/lbl_sub_total"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTotal"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_120pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            style="@style/txtPoppinsmedium14" android:text="57000"/>
                    </LinearLayout>

                    
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end"
                        android:id="@+id/btnRemove"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_28pxv"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_3pxv"
                        android:paddingEnd="@dimen/_18pxh"
                        android:paddingStart="@dimen/_18pxh"
                        android:paddingTop="@dimen/_3pxv"
                        android:layout_marginTop="10dp"
                        android:singleLine="true"
                        android:text="Contact Supplier"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_11" />

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>