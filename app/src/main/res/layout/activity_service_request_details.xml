<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical">
    <FrameLayout android:id="@+id/frameStackview"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="center" android:layout_height="wrap_content"
            android:layout_width="match_parent">

            <FrameLayout android:id="@+id/frameRegistration"
                android:layout_height="wrap_content" android:layout_width="match_parent">

                <LinearLayout android:id="@+id/linearColumnviewTwo"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">


                    <FrameLayout android:id="@+id/frameStackviewTwo"
                        android:layout_height="match_parent" android:layout_width="match_parent">

                        <LinearLayout
                            android:id="@+id/linearColumnlogo"
                            style="@style/groupStylewhite_A700"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="top|center"
                            android:orientation="vertical"
                            android:paddingStart="@dimen/_27pxh"
                            android:paddingTop="@dimen/_17pxv"
                            android:paddingEnd="@dimen/_27pxh"
                            android:paddingBottom="@dimen/_17pxv">



                            <LinearLayout
                                android:id="@+id/linearColumnname"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_80pxv"
                                android:gravity="start"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/txtName"
                                    style="@style/txtPoppinsregular15_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_3pxh"
                                    android:ellipsize="end"
                                    android:singleLine="true"
                                    android:text="@string/lbl_name" />

                                <EditText
                                    android:id="@+id/etName"
                                    style="@style/etRoundedOutline"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_3pxh"
                                    android:layout_marginTop="@dimen/_15pxv"
                                    android:background="@drawable/rectangle_border_blue_gray_100_radius_5"
                                    android:ellipsize="end"
                                    android:hint="@string/lbl_muhammed_suhail"
                                    android:textColor="@color/black"
                                    android:inputType="text"
                                    android:letterSpacing="0.04"
                                    android:paddingStart="@dimen/_19pxh"
                                    android:paddingTop="@dimen/_15pxv"
                                    android:paddingEnd="@dimen/_19pxh"
                                    android:paddingBottom="@dimen/_15pxv"
                                    android:singleLine="true"
                                    android:textColorHint="@color/gray_400"
                                    tools:ignore="Autofill" />
                            </LinearLayout>



                            <LinearLayout
                                android:id="@+id/linearColumnmobile"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_21pxv"
                                android:gravity="start"
                                android:orientation="vertical"
                                android:paddingStart="@dimen/_1pxh"
                                android:paddingEnd="@dimen/_1pxh">

                                <TextView
                                    android:id="@+id/txtMobile"
                                    style="@style/txtPoppinsregular15_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:ellipsize="end"
                                    android:singleLine="true"
                                    android:text="@string/lbl_mobile" />

                                <EditText
                                    android:id="@+id/etMobileNo"
                                    style="@style/etRoundedOutline"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_3pxh"
                                    android:layout_marginTop="@dimen/_15pxv"
                                    android:background="@drawable/rectangle_border_blue_gray_100_radius_5"
                                    android:ellipsize="end"
                                    android:textColor="@color/black"
                                    android:hint="@string/lbl_968_1234567896"
                                    android:inputType="text"
                                    android:letterSpacing="0.04"
                                    android:paddingStart="@dimen/_19pxh"
                                    android:paddingTop="@dimen/_15pxv"
                                    android:paddingEnd="@dimen/_19pxh"
                                    android:paddingBottom="@dimen/_15pxv"
                                    android:singleLine="true"
                                    android:textColorHint="@color/gray_400"
                                    tools:ignore="Autofill" />
                            </LinearLayout>



                            <LinearLayout
                                android:id="@+id/linearColumnBrand"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_17pxv"
                                android:gravity="start"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/b"
                                    style="@style/txtPoppinsregular15_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:ellipsize="end"
                                    android:singleLine="true"
                                    android:text="Car Brand" />


                                <EditText
                                    android:id="@+id/etBrand"
                                    style="@style/etRoundedOutline"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_3pxh"
                                    android:layout_marginTop="@dimen/_15pxv"
                                    android:background="@drawable/rectangle_border_blue_gray_100_radius_5"
                                    android:ellipsize="end"
                                    android:textColor="@color/black"
                                    android:hint="@string/lbl_brand"
                                    android:inputType="text"
                                    android:letterSpacing="0.04"
                                    android:paddingStart="@dimen/_19pxh"
                                    android:paddingTop="@dimen/_15pxv"
                                    android:paddingEnd="@dimen/_19pxh"
                                    android:paddingBottom="@dimen/_15pxv"
                                    android:singleLine="true"
                                    android:textColorHint="@color/gray_400"
                                    tools:ignore="Autofill" />

                                <LinearLayout
                                    android:id="@+id/linearColumnModel"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_17pxv"
                                    android:gravity="start"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/m"
                                        style="@style/txtPoppinsregular15_1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:ellipsize="end"
                                        android:singleLine="true"
                                        android:text="Car Model" />


                                    <EditText
                                        android:id="@+id/etModel"
                                        style="@style/etRoundedOutline"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_3pxh"
                                        android:layout_marginTop="@dimen/_15pxv"
                                        android:background="@drawable/rectangle_border_blue_gray_100_radius_5"
                                        android:ellipsize="end"
                                        android:textColor="@color/black"
                                        android:hint="Model"
                                        android:inputType="text"
                                        android:letterSpacing="0.04"
                                        android:paddingStart="@dimen/_19pxh"
                                        android:paddingTop="@dimen/_15pxv"
                                        android:paddingEnd="@dimen/_19pxh"
                                        android:paddingBottom="@dimen/_15pxv"
                                        android:singleLine="true"
                                        android:textColorHint="@color/gray_400"
                                        tools:ignore="Autofill" />

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/linearColumnvar"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_17pxv"
                                    android:gravity="start"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/txt"
                                        style="@style/txtPoppinsregular15_1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:ellipsize="end"
                                        android:singleLine="true"
                                        android:text="Car Variant" />


                                    <EditText
                                        android:id="@+id/etVariant"
                                        style="@style/etRoundedOutline"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_3pxh"
                                        android:layout_marginTop="@dimen/_15pxv"
                                        android:background="@drawable/rectangle_border_blue_gray_100_radius_5"
                                        android:ellipsize="end"
                                        android:textColor="@color/black"
                                        android:hint="Variant"
                                        android:inputType="text"
                                        android:letterSpacing="0.04"
                                        android:paddingStart="@dimen/_19pxh"
                                        android:paddingTop="@dimen/_15pxv"
                                        android:paddingEnd="@dimen/_19pxh"
                                        android:paddingBottom="@dimen/_15pxv"
                                        android:singleLine="true"
                                        android:textColorHint="@color/gray_400"
                                        tools:ignore="Autofill" />

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linearRowlogo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_17pxv"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/txtLogo"
                                    style="@style/txtPoppinsregular15_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:ellipsize="end"
                                    android:text="@string/msg_the_service_you" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linearRowdslrcameraon"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="10">

                                <EditText
                                    android:id="@+id/etService"
                                    style="@style/etRoundedOutline"

                                    android:layout_width="0dp"
                                    android:layout_weight="8"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_3pxh"
                                    android:layout_marginTop="@dimen/_15pxv"
                                    android:background="@drawable/rectangle_border_blue_gray_100_radius_5"
                                    android:ellipsize="end"
                                    android:hint="@string/lbl_service_details"
                                    android:textColor="@color/black"
                                    android:inputType="text"
                                    android:letterSpacing="0.04"
                                    android:paddingStart="@dimen/_19pxh"
                                    android:paddingTop="@dimen/_15pxv"
                                    android:paddingEnd="@dimen/_19pxh"
                                    android:paddingBottom="@dimen/_15pxv"
                                    android:singleLine="true"
                                    android:textColorHint="@color/gray_400"
                                    tools:ignore="Autofill" />

                                <FrameLayout
                                    android:id="@+id/frameStackdslrcamera"
                                    style="@style/groupStylegray_50_01"
                                    android:layout_width="0dp"
                                    android:layout_height="@dimen/_54pxv"
                                    android:layout_gravity="bottom"
                                    android:layout_marginStart="10dp"
                                    android:layout_weight="2">

                                    <ImageView
                                        android:id="@+id/imageView"
                                        android:layout_width="@dimen/_26pxh"
                                        android:layout_height="@dimen/_26pxh"
                                        android:layout_gravity="center"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/img_dslr_camera_1"
                                        tools:ignore="ContentDescription"
                                        tools:src="@drawable/img_dslr_camera_1" />
                                </FrameLayout>
                            </LinearLayout>



                            <LinearLayout
                                android:id="@+id/linearColumndescription"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_17pxv"
                                android:gravity="start"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/txtDescription"
                                    style="@style/txtPoppinsregular15_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:ellipsize="end"
                                    android:singleLine="true"
                                    android:text="@string/lbl_description" />


                                <EditText
                                    android:id="@+id/etDescription"
                                    style="@style/etRoundedOutline"
                                    android:layout_width="match_parent"
                                    android:layout_height="150dp"
                                    android:layout_marginStart="@dimen/_3pxh"
                                    android:layout_marginTop="@dimen/_15pxv"
                                    android:background="@drawable/rectangle_border_blue_gray_100_radius_5"
                                    android:ellipsize="end"
                                    android:hint="@string/lbl_description"
                                    android:textColor="@color/black"
                                    android:letterSpacing="0.04"
                                    android:paddingStart="@dimen/_19pxh"
                                    android:paddingTop="@dimen/_15pxv"
                                    android:paddingEnd="@dimen/_19pxh"
                                    android:paddingBottom="@dimen/_15pxv"
                                    android:singleLine="true"
                                    android:textColorHint="@color/gray_400"
                                    tools:ignore="Autofill"
                                    android:inputType="textMultiLine"
                                    android:gravity="top|start"
                                    android:scrollbars="vertical"/>

                            </LinearLayout>
                            <LinearLayout android:gravity="center_vertical"
                                android:id="@+id/linearRowcancel"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_30pxh"
                                android:layout_marginStart="@dimen/_30pxh"
                                android:layout_marginTop="@dimen/_32pxv"
                                android:weightSum="10"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <androidx.appcompat.widget.AppCompatButton
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:id="@+id/btnCancel"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_38pxv"
                                    android:layout_width="0dp"
                                    android:layout_weight="5"
                                    android:paddingBottom="@dimen/_10pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_30pxh"
                                    android:paddingTop="@dimen/_10pxv"
                                    android:singleLine="true"
                                    android:text="@string/lbl_cancel"
                                    android:textAllCaps="false"
                                    style="@style/btnSolidRounded_1"/>
                                <androidx.appcompat.widget.AppCompatButton
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:id="@+id/btnSubmit"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_38pxv"
                                    android:layout_marginStart="@dimen/_9pxh"
                                    android:layout_width="0dp"
                                    android:layout_weight="5"
                                    android:paddingBottom="@dimen/_10pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_30pxh"
                                    android:paddingTop="@dimen/_10pxv"
                                    android:singleLine="true"
                                    android:text="@string/lbl_submit"
                                    style="@style/btnSolidRounded_2" />
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
            </FrameLayout>
        </ScrollView>
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:background="@color/amber_500_02" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowdown"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:padding="10dp"
                android:weightSum="10"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowdownOne"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:src="@drawable/img_arrow_down_black_900"
                    android:layout_weight="1"
                    tools:ignore="ContentDescription" android:layout_gravity="center_vertical"/>
                <TextView
                    android:id="@+id/txtWorkshop"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="8"
                    android:text="@string/book_service"
                    style="@style/txtPoppinssemibold18" android:textAlignment="center"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
    </FrameLayout>
</LinearLayout>
