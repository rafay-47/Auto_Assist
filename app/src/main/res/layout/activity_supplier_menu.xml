<?xml version="1.0" encoding="UTF-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawerLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white_A700">
    <LinearLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        style="@style/groupStylewhite_A700">

        <FrameLayout android:id="@+id/frameStackoutputOne"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_822pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:background="@drawable/img_group_129"
                android:backgroundTint="@color/white_A700"
                tools:ignore="ContentDescription" />
            <LinearLayout android:id="@+id/linearColumnoutput"
                android:layout_gravity="top"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    app:contentInsetStart="0dp" style="@style/groupStyleamber_500_4">
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRowoutputOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_23pxh"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:weightSum="100"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <FrameLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="20">
                            <ImageView android:id="@+id/imageOutputOne"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_35pxh"
                                android:layout_width="@dimen/_35pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_output_onlinegiftools"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_output_onlinegiftools"/>
                        </FrameLayout>
                        <TextView
                            android:id="@+id/txtAutoassist"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_weight="60"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@string/lbl_auto_assist"
                            android:textAlignment="center"
                            android:gravity="center"
                            android:scaleType="fitXY"
                            style="@style/txtPoppinsbold16"
                            tools:text="@string/lbl_auto_assist"/>
                        <RelativeLayout
                            android:layout_width="@dimen/_35pxh"
                            android:layout_height="wrap_content"
                            android:layout_weight="20">

                            <ImageView
                                android:id="@+id/profilePic"
                                android:layout_width="@dimen/_35pxh"
                                android:layout_height="@dimen/_30pxh"
                                android:layout_alignParentEnd="true"
                                android:layout_centerVertical="true"
                                android:layout_marginEnd="0dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/baseline_person_24"
                                android:backgroundTint="@color/white"
                                tools:ignore="ContentDescription" />
                        </RelativeLayout>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>

                <LinearLayout
                    android:id="@+id/linearLOGIN"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_margin="20dp"
                    android:orientation="vertical"
                    style="@style/groupStylecornerRadius">

                    <LinearLayout android:id="@+id/linearColumn"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_20pxv"
                        android:paddingEnd="@dimen/_33pxh"
                        android:paddingStart="@dimen/_33pxh"
                        android:paddingTop="@dimen/_20pxv" >
                        <ImageView android:id="@+id/imageRequestImage"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_161pxh"
                            android:layout_width="@dimen/_282pxh"
                            android:scaleType="fitXY"
                            android:background="@drawable/img_request_parts_1"
                            tools:ignore="ContentDescription" />
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end" android:gravity="center"
                            android:id="@+id/btnAddParts"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_48pxv"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_10pxv"
                            android:paddingTop="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@string/add_parts"
                            android:textAllCaps="false"
                            style="@style/btnGradientRounded" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:layout_margin="20dp"
                    android:orientation="vertical"
                    style="@style/groupStylecornerRadius">

                    <LinearLayout android:id="@+id/linearColumn1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:paddingBottom="@dimen/_20pxv"
                        android:paddingEnd="@dimen/_33pxh"
                        android:paddingStart="@dimen/_33pxh"
                        android:paddingTop="@dimen/_20pxv" >
                        <ImageView android:id="@+id/imageRequestImage1"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_161pxh"
                            android:layout_width="@dimen/_232pxh"
                            android:scaleType="fitXY"
                            android:background="@drawable/manage_parts"
                            tools:ignore="ContentDescription" />
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end" android:gravity="center"
                            android:id="@+id/btnManageParts"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_48pxv"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_10pxv"
                            android:paddingTop="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="Manage Parts"
                            android:textAllCaps="false"
                            style="@style/btnGradientRounded" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </FrameLayout>
    </LinearLayout>

    <com.google.android.material.navigation.NavigationView
    android:id="@+id/navigationView"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:layout_gravity="start"
    android:background="@color/black_900_26"
    app:headerLayout="@layout/nav_header"
    app:itemBackground="@drawable/nav_selected_item"
    app:itemTextColor="@color/white_A700"
    app:itemIconTint="@color/amber_500_02"
    app:menu="@menu/supplier_nav_menu" />

</androidx.drawerlayout.widget.DrawerLayout>
