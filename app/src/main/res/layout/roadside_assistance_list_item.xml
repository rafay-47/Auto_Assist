<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/_340pxh"
    android:layout_height="@dimen/_290pxv"
    android:layout_margin="10dp"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="@drawable/layer_list_bg_white_a700_19_border_blue_gray_100_radius_5">

    <FrameLayout android:id="@+id/frameStackenquiryon"
        android:layout_gravity="center_horizontal"
        android:layout_height="match_parent"
        android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearColumnenquiryon"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_4pxh"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_22pxv"
            android:paddingEnd="@dimen/_17pxh"
            android:paddingStart="@dimen/_17pxh"
            android:paddingTop="@dimen/_22pxv" style="@style/groupStylewhite_3">
            <LinearLayout android:id="@+id/linearRowenquiryon"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txt2"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:singleLine="true"
                    style="@style/txtPoppinsbold12" android:text="Name:"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPerson"
                    android:gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{inquiryHistoryVM.inquiryHistoryModel.txtWorkshop}"
                    style="@style/txtPoppinsmedium14" tools:text="@string/lbl_workshop"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowdate"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txt1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:singleLine="true"
                    style="@style/txtPoppinsbold12" android:text="Contact No:"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtContact"
                    android:gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    style="@style/txtPoppinsmedium14" android:text="lbl_20_11_2021"/>
            </LinearLayout>

            <LinearLayout android:id="@+id/linearRowdate1"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txt4"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:singleLine="true"
                    style="@style/txtPoppinsbold12" android:text="Location:"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLocation"
                    android:gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    style="@style/txtPoppinsmedium14" android:text="lbl_20_11_2021"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView android:ellipsize="end"
                    android:id="@+id/txt"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:singleLine="true"
                    style="@style/txtPoppinsbold12" android:text="Issue:"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtService"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:layout_gravity="bottom"

                    style="@style/txtPoppinsmedium14" android:text="msg_dft_water_pump"/>
            </LinearLayout>

            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end"
                android:layout_gravity="bottom"
                android:id="@+id/btnViewDetails"
                android:layout_height="@dimen/_38pxv"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_212pxh"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_13pxh"
                android:paddingStart="@dimen/_13pxh"
                android:paddingTop="@dimen/_5pxv"
                android:singleLine="true"
                android:text="Respond to Request"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_11" />
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumnrequestid"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowrequestid"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_19pxh"
                android:paddingStart="@dimen/_19pxh"
                android:paddingTop="@dimen/_10pxv" style="@style/groupStyleamber_500cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCar"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:singleLine="true"
                    style="@style/txtPoppinssemibold14" android:text="Car:"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCarName"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    style="@style/txtPoppinssemibold14_1" android:text="lbl_ab899395"/>
            </LinearLayout>
            <ImageView android:gravity="center_horizontal"
                android:id="@+id/imageImageOne"
                android:layout_height="@dimen/_36pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_group_501"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group_501"/>
        </LinearLayout>
    </FrameLayout>

</LinearLayout>